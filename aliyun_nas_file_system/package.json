{
  "name": "ServerlessDevsComponent",
  "version": "0.0.1",
  "description": "This is a component demo for Serverless Devs Tool ",
  "autoInstall": false,
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write src/",
    "prebuild": "npm i && rimraf dist && npm run format && rimraf node_modules && npm i && npm link base_aliyun_resource",
    "build": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --outfile=dist/index.js"
  },
  "dependencies": {
    "@serverless-devs/component-interface": "^0.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "esbuild": "^0.13.8",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.2"
  }
}